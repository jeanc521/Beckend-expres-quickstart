// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id Int @id @default(autoincrement())
  name String @unique
  email String 
  passaword String
}

model List {
  id Int @id @default(autoincrement())
  description String
  price Float
  quanity Int 
  image String
}